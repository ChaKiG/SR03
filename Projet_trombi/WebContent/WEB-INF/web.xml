<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>index</servlet-name>
    <jsp-file>/index.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>index</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SousStructureServlet</servlet-name>
    <servlet-class>trombi.SousStructureServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SousStructureServlet</servlet-name>
    <url-pattern>/SousStructureServlet/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PersonneServlet</servlet-name>
    <servlet-class>trombi.PersonneServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PersonneServlet</servlet-name>
    <url-pattern>/PersonneServlet/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>StructureServlet</servlet-name>
    <servlet-class>trombi.StructureServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StructureServlet</servlet-name>
    <url-pattern>/StructureServlet/*</url-pattern>
  </servlet-mapping>
</web-app>